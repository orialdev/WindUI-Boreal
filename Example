local Notify_Section = Tab_Notify:Section({
    Title = "Notification Tester",
    Desc = "Test various notification types",
    Icon = "bell-ring",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

Notify_Section:Space({})

Notify_Section:Button({
    Title = "Send Basic Notify",
    Desc = "Standard Info",
    Icon = "info",
    Callback = function()
        WindUI:Notify({
            Title = "Information",
            Content = "This is a standard notification message.",
            Icon = "info",
            Duration = 5,
        })
    end,
})

Notify_Section:Space({})

Notify_Section:Button({
    Title = "Send Warning Notify",
    Desc = "Alert/Warning",
    Icon = "triangle-alert",
    Callback = function()
        WindUI:Notify({
            Title = "Warning System",
            Content = "Something requires your attention.",
            Icon = "triangle-alert",
            Duration = 5,
        })
    end,
})

Notify_Section:Space({})

Notify_Section:Button({
    Title = "Interactive Notify",
    Desc = "With Buttons",
    Icon = "mouse-pointer-click",
    Callback = function()
        WindUI:Notify({
            Title = "Confirmation",
            Content = "Do you want to enable the feature?",
            Icon = "info",
            Duration = 8,
            Buttons = {
                {
                    Title = "Yes",
                    Callback = function()
                        WindUI:Notify({
                            Title = "Success",
                            Content = "Feature enabled!",
                            Icon = "check",
                        })
                    end,
                },
                {
                    Title = "No",
                    Callback = function()
                        print("User clicked No")
                    end,
                }
            }
        })
    end,
})

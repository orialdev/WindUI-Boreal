local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/orialdev/WindUI-Boreal/main/WindUI%20Boreal"))()

local Window = WindUI:CreateWindow({
    Title = "WindUI Boreal",
    Size = UDim2.fromOffset(740, 560),
    Icon = "sparkles",
    ModernLayout = true
})

local SidebarLabel = nil
local SidebarButton = nil
local SidebarSpaceFrame = nil
local SidebarDividerFrame = nil
local SidebarPlaceholderLabel = nil

local function CreateSidebarElements()
    if not SidebarLabel then
        SidebarLabel = Window:SideBarLabel({
            Title = "SideBarLabel",
            Icon = "tag",
        })
    end

    if not SidebarButton then
        SidebarButton = Window:SideBarButton({
            Title = "SideBarButton",
            Icon = "locate-fixed",
            Variant = "Secondary",
            Callback = function()
                WindUI:Notify({
                    Title = "Notify",
                    Content = "SideBarButton: CenterWindow",
                    Icon = "bell",
                    Duration = 4,
                })
                Window:SetToTheCenter()
            end,
        })
    end

    if not SidebarSpaceFrame then
        SidebarSpaceFrame = Window:SideBarSpace({
            Columns = 2,
        })
    end

    if not SidebarDividerFrame then
        SidebarDividerFrame = Window:SideBarDivider({
            Title = "SideBarDivider",
            TitleAlignment = "Left",
        })
    end
end

local function DestroySidebarElements()
    if SidebarLabel then
        SidebarLabel:Destroy()
        SidebarLabel = nil
    end

    if SidebarButton then
        SidebarButton:Destroy()
        SidebarButton = nil
    end

    if SidebarSpaceFrame then
        SidebarSpaceFrame:Destroy()
        SidebarSpaceFrame = nil
    end

    if SidebarDividerFrame then
        SidebarDividerFrame:Destroy()
        SidebarDividerFrame = nil
    end

    if SidebarPlaceholderLabel then
        SidebarPlaceholderLabel:Destroy()
        SidebarPlaceholderLabel = nil
    end
end

CreateSidebarElements()

local Tab_MultiSection = Window:Tab({
    Title = "MultiSection",
    Icon = "layers",
})

Tab_MultiSection:Space({})

local MultiSection_Element = Tab_MultiSection:MultiSection({
    Title = "MultiSection",
    Desc = "MultiSection",
    Icon = "layout-grid",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

Tab_MultiSection:Space({})

local MultiSection_TabA = MultiSection_Element:Tab({
    Title = "Tab A",
    Desc = "Tab A",
    Icon = "square",
    Selected = true,
})

local MultiSection_TabB = MultiSection_Element:Tab({
    Title = "Tab B",
    Desc = "Tab B",
    Icon = "square-check",
})

MultiSection_TabA:Space({})

MultiSection_TabA:Paragraph({
    Title = "Info",
    Desc = "MultiSection Tab A",
    Text = "This is Tab A content.",
})

MultiSection_TabA:Space({})

MultiSection_TabA:Toggle({
    Title = "Enable Feature",
    Desc = "Toggle",
    Value = false,
    Callback = function(v)
        print("Enable Feature:", v)
    end,
})

MultiSection_TabA:Space({})

MultiSection_TabA:Button({
    Title = "Print Action",
    Desc = "Button",
    Icon = "mouse-pointer-click",
    Callback = function()
        print("Print Action clicked")
    end,
})

MultiSection_TabA:Space({})

MultiSection_TabB:Space({})

MultiSection_TabB:Slider({
    Title = "Adjust Value",
    Desc = "Slider",
    Value = { Min = 0, Max = 100, Default = 50 },
    Callback = function(v)
        print("Adjust Value:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_TabB:Input({
    Title = "Set Text",
    Desc = "Input",
    Value = "",
    Placeholder = "Type here",
    Callback = function(v)
        print("Set Text:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_TabB:Dropdown({
    Title = "Select Option",
    Desc = "Dropdown",
    Values = { "Option 1", "Option 2", "Option 3" },
    Value = "Option 1",
    Callback = function(v)
        print("Select Option:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_Element:SetIcon("layers-2")
MultiSection_Element:SetTitle("MultiSection")
MultiSection_Element:SetDesc("MultiSection")

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Select Tab A",
    Desc = "SelectTab",
    Icon = "arrow-left",
    Callback = function()
        MultiSection_Element:SelectTab(1, true)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Select Tab B",
    Desc = "SelectTab",
    Icon = "arrow-right",
    Callback = function()
        MultiSection_Element:SelectTab(2, true)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Open MultiSection",
    Desc = "Open",
    Icon = "chevron-down",
    Callback = function()
        MultiSection_Element:Open(true)
    end,
})


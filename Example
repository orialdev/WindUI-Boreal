    Desc = "ToggleKeybind:SetKeybind",
    Icon = "mouse-pointer-click",
    Callback = function()
        ToggleKeybind_Element:SetKeybind("MouseLeft")
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
    Title = "Set On",
    Desc = "ToggleKeybind:Set(true)",
    Icon = "check",
    Callback = function()
        ToggleKeybind_Element:Set(true, true, false)
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
    Title = "Set Off",
    Desc = "ToggleKeybind:Set(false)",
    Icon = "x",
    Callback = function()
        ToggleKeybind_Element:Set(false, true, false)
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
    Title = "Highlight",
    Desc = "ToggleKeybind:Highlight",
    Icon = "sparkles",
    Callback = function()
        ToggleKeybind_Element:Highlight()
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
    Title = "Lock",
    Desc = "ToggleKeybind:Lock",
    Icon = "lock",
    Callback = function()
        ToggleKeybind_Element:Lock()
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
    Title = "Unlock",
    Desc = "ToggleKeybind:Unlock",
    Icon = "lock-open",
    Callback = function()
        ToggleKeybind_Element:Unlock()
    end,
})

Tab_ToggleKeybind:Space({})


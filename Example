local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/orialdev/WindUI-Boreal/main/WindUI%20Boreal"))()

local Window = WindUI:CreateWindow({
    Title = "WindUI Boreal",
    Size = UDim2.fromOffset(740, 560),
    Icon = "sparkles",
    ModernLayout = true,
    Watermark = {
        Enabled = true,
        Text = "WindUI Boreal | Example",
        Opacity = 0.45,
        Position = "bottom-right", -- top-left/top-center/top-right/center/.../bottom-right
        Size = 14,
        Padding = 12,
        Offset = Vector2.new(0, 0),
        -- Color = Color3.fromRGB(255, 255, 255), -- optional
        -- RichText = false, -- optional
    },
})

Window:Divider({
    Title = "Example",
    TitleAlignment = "Left",
    Thickness = 2,
    TitleSpacing = 10,
    Color = "#8EA1B5",
    TitleColor = "Text",
})

Window:SideBarLabel({
    Title = "Example",
})

Window:SideBarButton({
    Title = "Button",
    Icon = "refresh-ccw",
    Callback = function()
        print("Sidebar button clicked")
    end
})

local Tab_MultiSection = Window:Tab({
    Title = "MultiSection",
    Icon = "layers",
})

Tab_MultiSection:Space({})

local MultiSection_Element = Tab_MultiSection:MultiSection({
    Title = "MultiSection",
    Desc = "MultiSection",
    Icon = "layout-grid",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

Tab_MultiSection:Space({})

local MultiSection_TabA = MultiSection_Element:Tab({
    Title = "Tab A",
    Desc = "Tab A",
    Icon = "square",
    Selected = true,
})

local MultiSection_TabB = MultiSection_Element:Tab({
    Title = "Tab B",
    Desc = "Tab B",
    Icon = "square-check",
})

MultiSection_TabA:Space({})

MultiSection_TabA:Paragraph({
    Title = "Info",
    Desc = "MultiSection Tab A",
    Text = "This is Tab A content.",
})

MultiSection_TabA:Space({})

MultiSection_TabA:Toggle({
    Title = "Enable Feature",
    Desc = "Toggle",
    Value = false,
    Callback = function(v)
        print("Enable Feature:", v)
    end,
})

MultiSection_TabA:Space({})

MultiSection_TabA:Button({
    Title = "Print Action",
    Desc = "Button",
    Icon = "mouse-pointer-click",
    Callback = function()
        print("Print Action clicked")
    end,
})

MultiSection_TabA:Space({})

MultiSection_TabB:Space({})

MultiSection_TabB:Slider({
    Title = "Adjust Value",
    Desc = "Slider",
    Value = { Min = 0, Max = 100, Default = 50 },
    Callback = function(v)
        print("Adjust Value:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_TabB:Input({
    Title = "Set Text",
    Desc = "Input",
    Value = "",
    Placeholder = "Type here",
    Callback = function(v)
        print("Set Text:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_TabB:Dropdown({
    Title = "Select Option",
    Desc = "Dropdown",
    Values = { "Option 1", "Option 2", "Option 3" },
    Value = "Option 1",
    Callback = function(v)
        print("Select Option:", v)
    end,
})

MultiSection_TabB:Space({})

MultiSection_Element:SetIcon("layers-2")
MultiSection_Element:SetTitle("MultiSection")
MultiSection_Element:SetDesc("MultiSection")

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Select Tab A",
    Desc = "SelectTab",
    Icon = "arrow-left",
    Callback = function()
        MultiSection_Element:SelectTab(1, true)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Select Tab B",
    Desc = "SelectTab",
    Icon = "arrow-right",
    Callback = function()
        MultiSection_Element:SelectTab(2, true)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Open MultiSection",
    Desc = "Open",
    Icon = "chevron-down",
    Callback = function()
        MultiSection_Element:Open(true)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Close MultiSection",
    Desc = "Close",
    Icon = "chevron-left",
    Callback = function()
        MultiSection_Element:Close(false)
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Lock MultiSection",
    Desc = "Lock",
    Icon = "lock",
    Callback = function()
        MultiSection_Element:Lock()
    end,
})

Tab_MultiSection:Space({})

Tab_MultiSection:Button({
    Title = "Unlock MultiSection",
    Desc = "Unlock",
    Icon = "lock-open",
    Callback = function()
        MultiSection_Element:Unlock()
    end,
})

Tab_MultiSection:Space({})

local Tab_Divider = Window:Tab({
    Title = "Divider",
    Icon = "minus",
})

Tab_Divider:Space({})

local Divider_Element = Tab_Divider:Divider({
    Title = "Example",
    TitleAlignment = "Center",
    Thickness = 2,
    TitleSpacing = 10,
    Color = "#8EA1B5",
    TitleColor = "Text",
})

Tab_Divider:Space({})

local Tab_Video = Window:Tab({
    Title = "Video",
    Icon = "video",
})

Tab_Video:Space({})

local Video_Element = Tab_Video:Video({
    Video = "5608321996",
    Height = 220,
    Looped = true,
    Playing = false,
    Volume = 0.5,
    Visible = true,
    Radius = 16,
    Padding = 6,
})

Tab_Video:Space({})

Tab_Video:Button({
    Title = "Play",
    Desc = "Video:Play",
    Icon = "play",
    Callback = function()
        Video_Element:Play()
    end,
})

Tab_Video:Space({})

Tab_Video:Button({
    Title = "Pause",
    Desc = "Video:Pause",
    Icon = "pause",
    Callback = function()
        Video_Element:Pause()
    end,
})

Tab_Video:Space({})

Tab_Video:Toggle({
    Title = "Looped",
    Desc = "Video:SetLooped",
    Value = true,
    Callback = function(v)
        Video_Element:SetLooped(v)
    end,
})

Tab_Video:Space({})

Tab_Video:Slider({
    Title = "Volume",
    Desc = "Video:SetVolume",
    Value = { Min = 0, Max = 10, Default = 0.5 },
    Callback = function(v)
        Video_Element:SetVolume(v)
    end,
})

Tab_Video:Space({})

Tab_Video:Slider({
    Title = "Height",
    Desc = "Video:SetHeight",
    Value = { Min = 24, Max = 360, Default = 220 },
    Callback = function(v)
        Video_Element:SetHeight(math.floor(v + 0.5))
    end,
})

Tab_Video:Space({})

Tab_Video:Toggle({
    Title = "Visible",
    Desc = "Video:SetVisible",
    Value = true,
    Callback = function(v)
        Video_Element:SetVisible(v)
    end,
})

Tab_Video:Space({})

Tab_Video:Button({
    Title = "Set Video Source",
    Desc = "Video:SetVideo",
    Icon = "film",
    Callback = function()
        Video_Element:SetVideo("rbxassetid://5608403837")
    end,
})

Tab_Video:Space({})

local Tab_BackgroundVideo = Window:Tab({
    Title = "Background Video",
    Icon = "tv",
})

Tab_BackgroundVideo:Space({})

Tab_BackgroundVideo:Button({
    Title = "Set Background Video",
    Desc = "Window:SetBackgroundVideo",
    Icon = "image",
    Callback = function()
        Window:SetBackgroundVideo("5608403837")
    end,
})

Tab_BackgroundVideo:Space({})

Tab_BackgroundVideo:Button({
    Title = "Set Background Image",
    Desc = "Window:SetBackgroundImage",
    Icon = "image",
    Callback = function()
        Window:SetBackgroundImage("rbxassetid://8508980527")
        Window:SetBackgroundImageTransparency(0.35)
    end,
})

Tab_BackgroundVideo:Space({})

Tab_BackgroundVideo:Slider({
    Title = "Background Transparency",
    Desc = "Window:SetBackgroundTransparency",
    Value = { Min = 0, Max = 1, Default = 0.2 },
    Callback = function(v)
        Window:SetBackgroundTransparency(v)
    end,
})

Tab_BackgroundVideo:Space({})

local Tab_ButtonKeybind = Window:Tab({
    Title = "Button Keybind",
    Icon = "mouse-pointer-click",
})

Tab_ButtonKeybind:Space({})

local ButtonKeybind_Element = Tab_ButtonKeybind:ButtonKeybind({
    Title = "ButtonKeybind",
    Desc = "Press key or click row",
    Icon = "mouse-pointer-click",
    IconThemed = true,
    Color = Color3.fromRGB(0, 170, 255),
    Justify = "Between",
    IconAlign = "Right",
    Value = Enum.KeyCode.F,
    CanChange = true,
    Locked = false,
    LockedTitle = "ButtonKeybind",
    Callback = function(trigger)
        print("ButtonKeybind:", trigger or "MouseClick")
    end,
})

Tab_ButtonKeybind:Space({})

local ButtonKeybind_Locked = Tab_ButtonKeybind:ButtonKeybind({
    Title = "ButtonKeybind (Locked)",
    Desc = "LockedTitle enabled",
    Icon = "mouse-pointer-click",
    IconThemed = true,
    Color = Color3.fromRGB(0, 170, 255),
    Justify = "Between",
    IconAlign = "Right",
    Value = Enum.KeyCode.F,
    CanChange = true,
    Locked = true,
    LockedTitle = "ButtonKeybind",
    Callback = function(trigger)
        print("ButtonKeybind (Locked):", trigger or "MouseClick")
    end,
})

Tab_ButtonKeybind:Space({})

Tab_ButtonKeybind:Button({
    Title = "Highlight",
    Desc = "ButtonKeybind:Highlight",
    Icon = "highlighter",
    Callback = function()
        ButtonKeybind_Element:Highlight()
    end,
})

Tab_ButtonKeybind:Space({})

Tab_ButtonKeybind:Button({
    Title = "Set Key: MouseRight",
    Desc = "ButtonKeybind:Set",
    Icon = "mouse",
    Callback = function()
        ButtonKeybind_Element:Set("MouseRight")
    end,
})

Tab_ButtonKeybind:Space({})

Tab_ButtonKeybind:Button({
    Title = "Lock",
    Desc = "ButtonKeybind:Lock",
    Icon = "lock",
    Callback = function()
        ButtonKeybind_Element:Lock()
    end,
})

Tab_ButtonKeybind:Space({})

Tab_ButtonKeybind:Button({
    Title = "Unlock",
    Desc = "ButtonKeybind:Unlock",
    Icon = "lock-open",
    Callback = function()
        ButtonKeybind_Element:Unlock()
    end,
})

Tab_ButtonKeybind:Space({})

local Tab_ToggleKeybind = Window:Tab({
    Title = "Toggle Keybind",
    Icon = "toggle-left",
})

Tab_ToggleKeybind:Space({})

local ToggleKeybind_Element = Tab_ToggleKeybind:ToggleKeybind({
    Title = "ToggleKeybind",
    Desc = "Press keybind to toggle",
    Type = "Toggle",
    Value = false,
    Keybind = Enum.KeyCode.G,
    CanChange = true,
    Locked = false,
    LockedTitle = "ToggleKeybind",
    Callback = function(state)
        print("ToggleKeybind:", state)
    end,
})

Tab_ToggleKeybind:Space({})

local ToggleKeybind_Locked = Tab_ToggleKeybind:ToggleKeybind({
    Title = "ToggleKeybind (Locked)",
    Desc = "LockedTitle enabled",
    Type = "Toggle",
    Value = false,
    Keybind = Enum.KeyCode.G,
    CanChange = true,
    Locked = true,
    LockedTitle = "ToggleKeybind",
    Callback = function(state)
        print("ToggleKeybind (Locked):", state)
    end,
})

Tab_ToggleKeybind:Space({})

Tab_ToggleKeybind:Button({
